<ResourceDictionary x:Class="TestYourself.Assets.ButtonIcons"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<Viewbox x:Key="NextIcon">
		<Grid Height="100" Width="100">
			<Ellipse Stroke="{StaticResource PhoneChromeBrush}" StrokeThickness="5"/>
			<Rectangle HorizontalAlignment="Left" Height="42" Margin="18,31,0,0" Stroke="Black" StrokeThickness="0" VerticalAlignment="Top" Width="5" Fill="{StaticResource PhoneAccentBrush}"/>
			<Path Data="M299.134,162.984L334.087,194.984 299.134,226.984 299.134,162.984z" 
			Stretch="Uniform" 
			Fill="{StaticResource PhoneAccentBrush}" Margin="37,31,27.381,27" RenderTransformOrigin="0.5,0.5" UseLayoutRounding="False">
				<Path.RenderTransform>
					<TransformGroup>
						<TransformGroup.Children>
							<RotateTransform Angle="0" />
							<ScaleTransform ScaleX="1" ScaleY="1" />
						</TransformGroup.Children>
					</TransformGroup>
				</Path.RenderTransform>
			</Path>
			<Rectangle HorizontalAlignment="Left" Height="42" Margin="76,31,0,0" Stroke="Black" StrokeThickness="0" VerticalAlignment="Top" Width="5" Fill="{StaticResource PhoneChromeBrush}"/>
		</Grid>
	</Viewbox>

</ResourceDictionary>